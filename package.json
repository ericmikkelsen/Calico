{
  "name": "calico",
  "version": "1.0.0",
  "description": "Generates color information about color palette",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && mkdir public && node --experimental-modules index.mjs && npm run copy:assets",
    "clean": "rm -rf ./public",
    "copy:assets": "cp -a ./modules/ ./public/ && cp -a ./styles/ ./public/",
  },
